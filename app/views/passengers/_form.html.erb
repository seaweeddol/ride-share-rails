<%= form_with model: @passenger, class: 'create-passenger' do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
  </div>

  <div>
    <%= f.submit action_name, class: "passenger_button" %>
  </div>
<% end %>